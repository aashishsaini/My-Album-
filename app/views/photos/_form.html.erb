
<%= form_for([@cover, @cover.photos.build], :html => { :multipart => true }) do |f| %>
  <div class="field">
     <%= f.label :name %><br />
     <%= f.text_field :name %>
  </div>
<div class="field">
     <%= f.label :image_url %><br />
     <%= f.file_field :image_url %>
  
  
</div>  


<div class="actions">
     <%= f.submit %>
  </div>
<% end %>


